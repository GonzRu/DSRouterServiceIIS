<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<ErrorList>
    <!--ошибки разбиты по категориям
        0 - неопределенное значение
        1 - ошибок нет
        100-1000 - Общие ошибки
        1000-2000 - Ошибки конфигурирования
    -->
  <ErrorCategory name = "Общие">
        <Error code = "999" name = "Неизвествая ошибка" />
        <Error code = "998" name = "Запрос текста ошибки не может быть выполнен: указанный код не существует." />
  </ErrorCategory>
    <ErrorCategory name = "Ошибки конфигурирования">
        <Error code = "1999" name = "Неизвествая ошибка" />
        <Error code = "1001" name = "Тег не существует" />
  </ErrorCategory>
</ErrorList>