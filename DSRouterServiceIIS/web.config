    <trace autoflush="true" />
    
    <sources>
      <source name="System.ServiceModel"
               switchValue="Warning"
               propagateActivity="true">
        <listeners>
          <add name="sdt"
              type="System.Diagnostics.XmlWriterTraceListener"
              initializeData= "SdrConfigExample.e2e" />
